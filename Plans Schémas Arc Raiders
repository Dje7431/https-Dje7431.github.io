<script>
// Tri simple par clic sur l'en-t√™te
document.querySelectorAll("#bpTable th").forEach((th, idx) => {
  th.addEventListener("click", () => {
    const tbody = document.querySelector("#bpTable tbody");
    const rows = Array.from(tbody.querySelectorAll("tr"));
    const asc = !th.classList.contains("asc");
    document.querySelectorAll("#bpTable th").forEach(h => h.classList.remove("asc","desc"));
    th.classList.add(asc ? "asc" : "desc");
    rows.sort((a, b) => {
      const A = a.children[idx].innerText.trim().toLowerCase();
      const B = b.children[idx].innerText.trim().toLowerCase();
      return asc ? A.localeCompare(B) : B.localeCompare(A);
    });
    rows.forEach(r => tbody.appendChild(r));
  });
});
</script>



</body></html>
